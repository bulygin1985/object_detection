{
  "model": {
    "alpha": 1.0,
    "weights_path": null,
    "backbone": {
      "name": "efficientnet_b0",
      "pretrained_weights": "DEFAULT"
    },
    "head": {
      "filters_size": [
        256,
        128,
        64
      ],
      "head_layers": 3,
      "conv_bias": true
    }
  },
  "train": {
    "is_overfit": false,
    "lr": 2e-2,
    "lr_backbone": 2e-2,
    "lr_head": 2e-2,
    "lr_head_pretrain": 2e-2,
    "head_pretrain_epochs": 70,
    "backbone_trainable_params_patterns_include": null,
    "backbone_trainable_params_patterns_exclude": [
      "features.1",
      "features.2",
      "features.3",
      "features.4",
      "features.5"
    ],
    "batch_size": 32,
    "drop_last": true,
    "subset_len": null,
    "val_subset_len": null,
    "calculate_epoch_loss": true,
    "num_workers": 8,
    "save_best_model": true,
    "skip_save_best_model_epochs": 70,
    "weight_decay": 0.0001,
    "weight_decay_bias": false,
    "data_augmentation": {
      "random_flip_horizontal": true,
      "random_resize_crop_scale": [0.5, 1.0],
      "random_resize_crop_ratio": [0.6667, 1.5]
    },
    "lr_schedule": {
      "type": "step",
      "step": {
        "step_size": 1,
        "factor": 0.96,
        "last_epoch": -1
      },
      "multi_step": {
        "milestones": [30,45,60,75],
        "factor": 0.5  
      },
      "reduce_on_plato": {
        "patience": 3,
        "factor": 0.2,
        "min_lr": 1e-5
      }
    },
    "stop_criteria": {
      "loss": null,
      "epoch": 150
    }
  },
  "data": {
    "is_download":  false
  }
}
